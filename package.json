{
  "name": "@vsc_cnst/countries-info",
  "version": "1.0.0",
  "description": "A TypeScript library that provides detailed information about countries, including their names, languages, translations, and other metadata.",
  "author": "vsc",
  "license": "MIT",
  "main": "dist/index.ts",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": false,
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "target": [
      "esnext",
      "es2015"
    ],
    "dts": true,
    "minify": true,
    "tsconfig": "tsconfig.json"
  },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ts",
    "type",
    "types",
    "country",
    "countries",
    "info",
    "information",
    "code",
    "codes",
    "codes"
  ],
  "devDependencies": {
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  }
}
